{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "no-console": "off",
    "no-await-in-loop": "off"
  },
  "settings": {
    "import/resolver": "node"
  }
}
